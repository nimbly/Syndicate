{
    "name": "nimbly/syndicate",
    "description": "Simple queue consumer framework that supports message routing and dispatching.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@brentscheffler.com"
        }
    ],
    "require": {
		"php": "^8.2",
        "psr/container": "^1.0|^2.0",
        "nimbly/resolve": "^2.0",
		"pda/pheanstalk": "^5.0",
		"predis/predis": "^2.3",
		"google/cloud-pubsub": "^2.8",
		"microsoft/azure-storage-queue": "^1.3",
		"iron-io/iron_mq": "^4.0",
        "php-mqtt/client": "^2.2",
        "php-amqplib/php-amqplib": "^3.7",
		"aws/aws-sdk-php": "^3.336"
    },
    "require-dev": {
        "vimeo/psalm": "^4.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^6.0",
        "nimbly/carton": "^2.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Nimbly\\Syndicate\\": "src/",
			"Nimbly\\Syndicate\\Examples\\": "examples/"
        }
	},
	"config": {
		"platform": {
			"php": "8.2"
		}
